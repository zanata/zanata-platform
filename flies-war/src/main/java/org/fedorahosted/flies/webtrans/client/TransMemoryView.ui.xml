<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:f="urn:import:org.fedorahosted.flies.webtrans.client">
	<g:LayoutPanel>
		<g:layer top="0px" height="25px">
			<g:FlowPanel>
				<g:TextBox ui:field="tmTextBox" />
				<g:CheckBox ui:field="phraseButton"/>
				<g:Button ui:field="searchButton"/>
				<g:Button ui:field="clearButton"/>
			</g:FlowPanel>
		</g:layer>
		<g:layer top="25px" bottom="0px" >
			<g:ScrollPanel>
				<g:FlexTable ui:field="resultTable"/>
			</g:ScrollPanel>
		</g:layer>
	</g:LayoutPanel>
</ui:UiBinder> 
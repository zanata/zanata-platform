<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.divider {
			color: #e0e0e0;	
		}
		.inlineLabel {
			display: inline;	
		}
	</ui:style>
	<g:HTMLPanel>
		<nobr>
			<g:Anchor href="#" ui:field="prevEntryButton" />
			<span class="{style.divider}">|</span>
			<g:Anchor href="#" ui:field="nextEntryButton" />
			<span class="{style.divider}">|</span>
			<g:Anchor href="#" ui:field="prevFuzzyButton" />
			<span class="{style.divider}">|</span>
			<g:Anchor href="#" ui:field="nextFuzzyButton" />
			<span class="{style.divider}">|</span>
			<g:Anchor href="#" ui:field="prevUntranslatedButton" />
			<span class="{style.divider}">|</span>
			<g:Anchor href="#" ui:field="nextUntranslatedButton" />
			<span class="{style.divider}">|</span>
			<g:Label styleName="{style.inlineLabel}" ui:field="shortcutLabel" />
		</nobr>
	</g:HTMLPanel>
</ui:UiBinder> 
  "scripts": {
    "check": "check-node-version --node $(cat .nvmrc) --yarn $(cat .yarn-version) --print",
    "js": "yarn run check && webpack",
    "build": "yarn run check && node scripts/build",
    "draft": "yarn run check && node scripts/build --draft",
    "watch": "yarn run check && node scripts/dev-server",
    "watch:editor": "yarn run check && node scripts/dev-server --editor",
    "styleguide-server": "yarn run check && styleguidist server",
    "styleguide-build": "yarn run check && styleguidist build",
    "storybook-editor": "yarn run check && start-storybook -p 9001 -s ./dist --config-dir .storybook-editor",
    "storybook-frontend": "yarn run check && start-storybook -p 9001 -s ./dist --config-dir .storybook-frontend",
    "storybook-build": "yarn run check && build-storybook -s ./dist",
    "test": "yarn run check && jest --coverage",
    "test:watch": "yarn run check && jest --watch --coverage"
  },

    "collectCoverageFrom": [
      "app/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!app/**/*.story.js"
    ],
    "coverageReporters": [
      "cobertura",
      "html",
      "lcov",
      "text"
    ],
    "moduleNameMapper": {
      "^.+\\.(css)$": "./__tests__/mock/mockCss.js"
    },
    "transform": {
      ".*": "./node_modules/babel-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/mock"
    ],
    "unmockedModulePathPatterns": [
      "/node_modules",
      "/app"

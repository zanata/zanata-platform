    "webpack-merge": "1.1.1"
  },
  "dependencies": {
    "babel-polyfill": "6.9.1",
    "chart.js": "1.1.1",
    "combokeys": "2.4.6",
    "defined": "1.0.0",
    "file-saver": "1.3.0",
    "history": "2.1.2",
    "js-beautify": "1.6.4",
    "lodash": "4.13.1",
    "moment": "2.13.0",
    "moment-range": "2.2.0",
    "normalize.css": "3.0.1",
    "normalizr": "2.1.0",
    "react": "0.14.8",
    "react-autosuggest": "6.0.4",
    "react-bootstrap": "0.30.7",
    "react-chartjs": "0.4.0",
    "react-date-range": "0.2.4",
    "react-dom": "0.14.8",
    "react-helmet": "3.1.0",

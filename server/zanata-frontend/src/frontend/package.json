{
  "name": "zanata-frontend",
  "version": "1.0.0",
  "description": "Zanata front-end package",
  "main": "index.js",
  "scripts": {
    "js": "webpack",
    "build": "yarn build:icons && webpack -p --config webpack.prod.config.js --bail --display-error-details",
    "draft": "yarn build:icons && webpack --config webpack.draft.config.js --progress --bail --display-error-details",
    "watch": "webpack-dev-server -d --progress --config webpack.dev.config.js --progress --inline --hot --port 8000",
    "watch:editor": "mkdir -p dist && cp app/editor/index.html dist && webpack-dev-server -d --progress --config webpack.dev.config.js --inline --hot --content-base dist/",
    "build:icons": "yarn svg && yarn icons",
    "styleguide-server": "styleguidist server",
    "styleguide-build": "styleguidist build",
    "storybook": "start-storybook -p 9001 -s ./dist",
    "storybook-build": "build-storybook -s ./dist",
    "icons": "node scripts/createIconsComponent && node scripts/generateIconList",
    "svg": "svg-sprite -sD app/components/Icons --symbol-dest '' --ss icons.svg --si app/components/Icons/svgs/*.svg",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zanata/zanata-platform.git"
  },
  "engines": {
    "node": "5.6.0",
    "yarn": "0.18.1"
  },
  "license": "LGPL-2.1",
  "devDependencies": {
    "@kadira/storybook": "1.36.0",
    "autoprefixer-loader": "3.2.0",
    "babel-core": "6.9.1",
    "babel-eslint": "6.1.0",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-lodash": "2.1.0",
    "babel-plugin-react-intl": "2.1.3",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "classnames": "2.2.5",
    "css-loader": "0.23.1",
    "csso-loader": "0.0.6",
    "eslint": "2.13.1",
    "eslint-config-standard": "5.3.1",
    "eslint-config-standard-react": "2.5.0",
    "eslint-config-zanata": "2.0.1",
    "eslint-loader": "1.3.0",
    "eslint-plugin-no-console-log": "1.0.0",
    "eslint-plugin-promise": "1.3.2",
    "eslint-plugin-react": "4.2.3",
    "eslint-plugin-standard": "1.3.2",
    "extract-text-webpack-plugin": "1.0.1",
    "fake-zanata-server": "3.0.5",
    "faucet": "0.0.1",
    "isomorphic-fetch": "2.2.1",
    "jest-cli": "18.0.0",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "postcss-loader": "0.8.2",
    "react-addons-test-utils": "0.14.8",
    "react-addons-update": "0.14.8",
    "react-intl": "2.1.2",
    "react-overlays": "0.6.10",
    "react-sidebar": "2.3.0",
    "react-split-pane": "0.1.42",
    "react-styleguidist": "2.3.1",
    "react-transform-hmr": "1.0.4",
    "redux-simple-router": "2.0.4",
    "reselect": "1.1.0",
    "rework-calc": "1.1.0",
    "rework-color-function": "1.2.1",
    "rework-custom-media": "0.1.1",
    "rework-ie-limits": "0.1.1",
    "rework-loader": "0.1.1",
    "rework-npm": "1.0.0",
    "rework-suit-conformance": "0.4.2",
    "rework-vars": "3.1.1",
    "style-loader": "0.13.1",
    "webpack": "1.13.0",
    "webpack-dev-middleware": "1.6.1",

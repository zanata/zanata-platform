<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="resources" type="org.zanata.webtrans.client.Resources" />
	<ui:style>
		.imageAnchor{
			cursor: pointer;
			}
	</ui:style>
	<g:HTMLPanel>
		<nobr>
			<g:Image resource="{resources.undo}" ui:field="undo" styleName="{style.imageAnchor}" visible ="false"/>
			<g:Image resource="{resources.undoDisabled}" ui:field="undoDisabled"  />
			<g:Image resource="{resources.redo}" ui:field="redo" styleName="{style.imageAnchor}" visible="false" />
			<g:Image resource="{resources.redoDisabled}" ui:field="redoDisabled"  />
		</nobr>
	</g:HTMLPanel>
</ui:UiBinder> 